{
  "name": "exotelis/contao-dive-information",
  "description": "Adds additional information to a member",
  "type": "contao-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Sebastian Krah",
      "email": "exotelis@mailbox.org",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/Exotelis/contao-dive-information/issues",
    "source": "https://github.com/Exotelis/contao-dive-information"
  },
  "require": {
    "php": "^7.4 || ^8.0",
    "contao/core-bundle": "^4.13",
    "contao/newsletter-bundle": "^4.13",
    "phpoffice/phpspreadsheet": "^1.26"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.3.1"
  },
  "autoload": {
    "psr-4": {
      "Exotelis\\ContaoDiveInformationBundle\\": "src/"
    },
    "classmap": [
      "src/Resources/contao/"
    ],
    "exclude-from-classmap": [
      "src/Resources/contao/config/",
      "src/Resources/contao/dca/",
      "src/Resources/contao/languages/",
      "src/Resources/contao/templates/"
    ]
  },
  "extra": {
    "contao-manager-plugin": "Exotelis\\ContaoDiveInformationBundle\\ContaoManager\\Plugin"
  }
}
